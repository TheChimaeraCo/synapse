{
  "name": "synapse",
  "version": "0.3.0",
  "private": true,
  "description": "Self-hosted AI command center. Manage AI agents across Telegram, Discord, WhatsApp, and web from a single dashboard.",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheChimaeraCo/synapse.git"
  },
  "homepage": "https://github.com/TheChimaeraCo/synapse",
  "bugs": {
    "url": "https://github.com/TheChimaeraCo/synapse/issues"
  },
  "license": "FSL-1.1-MIT",
  "author": "The Chimaera Company LLC",
  "scripts": {
    "env:pull": "node scripts/convex-env.js pull",
    "env:push": "node scripts/convex-env.js push",
    "dev": "node scripts/convex-env.js pull --silent && next dev",
    "build": "node scripts/convex-env.js pull --silent && next build",
    "start": "node scripts/convex-env.js pull --silent && next start",
    "init": "node scripts/init.js",
    "synapse": "node scripts/init.js",
    "dev:full": "node scripts/convex-env.js pull --silent && node scripts/dev.js",
    "stop": "node scripts/stop.js",
    "restart": "node scripts/init.js --skip-install --skip-build",
    "verify:deploy": "node scripts/deploy.js --verify-only",
    "deploy:convex": "node scripts/deploy.js",
    "deploy:convex:dry-run": "node scripts/deploy.js --dry-run",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@auth/core": "^0.41.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@mariozechner/pi-ai": "^0.54.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@sinclair/typebox": "^0.34.41",
    "@slack/web-api": "^7.14.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "discord.js": "^14.25.1",
    "grammy": "^1.40.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.624",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
